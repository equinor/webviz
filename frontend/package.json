{
    "name": "webviz",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": "^24.13.0",
        "npm": "^11.6.2"
    },
    "scripts": {
        "dev": "npm run generate:module-states-map && vite --host",
        "build": "npm run generate:module-states-map && tsc && NODE_OPTIONS='--max-old-space-size=8192' vite build",
        "preview": "vite preview",
        "generate:api": "openapi-ts",
        "generate:module-states-map": "node scripts/generate-module-states-map.js && eslint src/modules/ModuleSerializedStateMap.ts --fix",
        "generate-api": "openapi-ts",
        "typecheck": "tsc --noEmit",
        "lint": "eslint 'src/**/*.+(ts|tsx|js|jsx|json)' 'tests/**/*.+(ts|tsx|js|jsx|json)' --max-warnings=0",
        "lint:fix": "eslint 'src/**/*.+(ts|tsx|js|jsx|json)' 'tests/**/*.+(ts|tsx|js|jsx|json)' --fix",
        "depscheck": "dependency-cruise src",
        "validate": "npm run typecheck && npm run lint && npm run depscheck",
        "test:unit": "vitest run --coverage",
        "test:e2e": "playwright test --config=tests/e2e/_playwright.config.ts",
        "test:e2e:ui": "playwright test --config=tests/e2e/_playwright.config.ts -- --ui",
        "test:ct": "playwright test -c playwright.ct.config.ts",
        "test:ct:ui": "playwright test -c playwright.ct.config.ts --ui"
    },
    "-- Comments --": [
        "@hey-api/openapi-ts does not use semantic versioning, x.1.x can have breaking changes",
        "Some packages are dropping Node 18, and cannot be updated. This includes: `react@19`, `vite@17`, `@vitejs/plugin-react@5`, `vite-plugin-checker@0.10`, `glob@11`, dependency-cruiser@17",
        "By default we install deps with ^ versions. Any package with ~ versions tends to put breaking changes in minor updates",
        "`buffer` is installed as a workaround for issue in the map module. See webviz-subsurface-components issue #2540 for details",
        "ts-key-enum v2 and v3 are the same, expect that v3 exports it as const enum. We cannot use v3 since we compile typescript with isolatedModules=true"
    ],
    "dependencies": {
        "@equinor/eds-core-react": "~0.48.0",
        "@equinor/esv-intersection": "^3.1.4",
        "@mui/base": "^5.0.0-beta.3",
        "@mui/icons-material": "^5.14.9",
        "@tanstack/query-core": "^5.62.16",
        "@tanstack/react-query": "^5.63",
        "@tanstack/react-query-devtools": "^5.63",
        "@webviz/group-tree-plot": "^1.6.19",
        "@webviz/subsurface-viewer": "^1.15.13",
        "@webviz/well-completions-plot": "^1.8.9",
        "@webviz/well-log-viewer": "^2.6.10",
        "animate.css": "^4.1.1",
        "axios": "^1.8.3",
        "buffer": "^6.0.3",
        "comlink": "^4.4.2",
        "culori": "^4.0.2",
        "error-stack-parser": "^2.1.4",
        "geojson": "^0.5.0",
        "jotai": "^2.6.2",
        "jotai-effect": "^2.0.5",
        "jotai-tanstack-query": "~0.11.0",
        "lodash": "^4.17.23",
        "pixi.js": "^7.1.0",
        "rbush": "^4.0.1",
        "react": "^18.3.0",
        "react-dom": "^18.3.0",
        "react-plotly.js": "^2.6.0",
        "react-toastify": "^11.0.5",
        "simplify-js": "^1.2.4",
        "source-map-js": "^1.2.1",
        "ts-key-enum": "^2.0.13",
        "uuid": "^13.0.0",
        "wonka": "^6.3.4"
    },
    "overrides": {
        "@playwright/experimental-ct-core": {
            "vite": "$vite"
        },
        "react-plotly.js": {
            "plotly.js": "^3.3.1"
        },
        "@deck.gl/core": "~9.1.15",
        "@deck.gl/layers": "~9.1.15",
        "@deck.gl/aggregation-layers": "~9.1.15",
        "@deck.gl/extensions": "~9.1.15",
        "@deck.gl/geo-layers": "~9.1.15",
        "@deck.gl/json": "~9.1.15",
        "@deck.gl/mesh-layers": "~9.1.15",
        "@deck.gl/react": "~9.1.15"
    },
    "devDependencies": {
        "@hey-api/openapi-ts": "~0.92.4",
        "@playwright/experimental-ct-react": "~1.58.2",
        "@playwright/test": "~1.58.2",
        "@tailwindcss/vite": "^4.0.9",
        "@types/culori": "^4.0.0",
        "@types/geojson": "^7946.0.16",
        "@types/lodash-es": "^4.17.9",
        "@types/node": "^25.2.3",
        "@types/rbush": "^4.0.0",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@types/react-plotly.js": "^2.6.0",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^5.1.4",
        "@vitest/coverage-istanbul": "^4.0.18",
        "dependency-cruiser": "^16.10.4",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^10.0.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.1.0",
        "fast-glob": "^3.3.3",
        "glob": "^13.0.3",
        "globals": "^17.3.0",
        "prettier": "^3.5.2",
        "prettier-plugin-glsl": "^0.2.2",
        "sass-embedded": "^1.97.3",
        "tailwindcss": "^4.0.9",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.25.0",
        "vite": "^7.3.1",
        "vite-plugin-checker": "^0.12.0",
        "vite-plugin-glsl": "^1.3.1",
        "vitest": "^4.0.18"
    }
}
