{
    "compilerOptions": {
        "paths": {
            "@api": [
                "./src/api"
            ],
            "@framework/*": [
                "./src/framework/*"
            ],
            "@lib/*": [
                "./src/lib/*"
            ],
            "@modules/*": [
                "./src/modules/*"
            ],
            "@modules_shared/*": [
                "./src/modules/_shared/*"
            ]
        }
    }
}